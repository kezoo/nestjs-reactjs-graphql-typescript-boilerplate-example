{
  "name": "fe-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run clear_cache:eslint && cross-env WEBPACK_IS_DEV_SERVER=true NODE_ENV=development webpack serve --config webpack.config.babel.js",
    "build:dev": "npm run clean:build && webpack --config webpack.config.babel.js",
    "build:test": "npm run clean:build && cross-env NODE_ENV=test webpack --config webpack.config.babel.js",
    "build:production": "npm run clean:build && cross-env NODE_ENV=production webpack --config webpack.config.babel.js",
    "clean:build": "rm -rf dist",
    "lint": "./node_modules/.bin/eslint src --env-info --debug --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "./node_modules/.bin/eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clear_cache:eslint": "rm -rf node_modules/.cache/eslint-loader",
    "create:merge_request": "./node_modules/.bin/tsc CreateMergeRequest.ts"
  },
  "author": "",
  "license": "ISC",
  "resolutions": {
    "@types/react": "^18.0.0",
    "**/react": "18.1.0",
    "**/react-dom": "18.1.0"
  },
